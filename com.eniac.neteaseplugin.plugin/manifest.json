{
    "name": "NeteasePlugin",
    "uuid": "com.eniac.neteaseplugin",
    "version": "1.0.0",
    "author": "ENIAC",
    "entry": "backend/plugin.cjs",
    "description": "网易云音乐控制面板 - 通过 BetterNCM + FlexLink 实现通信",
    "repo": "https://github.com/ENIAC-Tech/NeteasePlugin.git",
    "sdk": {
        "version": "1.0.0"
    },
    "software": {
        "minimumVersion": "1.0.0"
    },
    "os": [
        {
            "platform": "mac",
            "minimumVersion": "10.15"
        },
        {
            "platform": "windows",
            "minimumVersion": "10"
        }
    ],
    "device": [
        {
            "name": "flexbar",
            "version": "1.0.0"
        }
    ],
    "configPage": "configPage",
    "keyLibrary": {
        "title": "$PluginName",
        "style": {
            "icon": "data:image/jpg;base64,"
        },
        "children": [
            {
                "title": "$NowPlaying.Title",
                "tip": "$NowPlaying.Tip",
                "cid": "com.eniac.neteaseplugin.nowplaying",
                "config": {
                    "keyType": "default",
                    "clickable": false,
                    "platform": ["windows", "mac"]
                },
                "style": {
                    "icon": "mdi mdi-music",
                    "width": 600,
                    "flags": ["disable-bg", "disable-fg", "disable-icon-sel", "disable-layout-sel"]
                },
                "data": {
                    "showTitle": true,
                    "showArtist": true,
                    "showAlbum": true,
                    "showProgress": true
                }
            },
            {
                "title": "$Lyric.Title",
                "tip": "$Lyric.Tip",
                "cid": "com.eniac.neteaseplugin.lyric",
                "config": {
                    "keyType": "default",
                    "clickable": false,
                    "platform": ["windows", "mac"]
                },
                "style": {
                    "icon": "mdi mdi-text",
                    "width": 480,
                    "flags": ["disable-bg", "disable-fg", "disable-icon-sel", "disable-layout-sel"]
                },
                "data": {
                    "showTranslation": true,
                    "highlightWord": true,
                    "backgroundColor": "#1a1a1a",
                    "primaryAlign": "left",
                    "primaryFontSize": 26,
                    "primaryColor": "#FFFFFF",
                    "highlightColor": "#C81919",
                    "primaryPaddingTop": 1,
                    "secondaryAlign": "left",
                    "secondaryFontSize": 22,
                    "secondaryColor": "#A0A0A0",
                    "secondaryPaddingTop": 30,
                    "paddingHorizontal": 10
                }
            },
            {
                "title": "$PlayMode.Title",
                "tip": "$PlayMode.Tip",
                "cid": "com.eniac.neteaseplugin.playmode",
                "config": {
                    "keyType": "multiState",
                    "platform": ["windows", "mac"]
                },
                "style": {
                    "icon": "mdi mdi-playlist-music",
                    "width": 240,
                    "multiStyle": [
                        {
                            "icon": "mdi mdi-playlist-music",
                            "fgColor": "#FFFFFF",
                            "title": "$PlayMode.Sequential"
                        },
                        {
                            "icon": "mdi mdi-repeat",
                            "fgColor": "#FFFFFF",
                            "title": "$PlayMode.ListLoop"
                        },
                        {
                            "icon": "mdi mdi-repeat-once",
                            "fgColor": "#FFFFFF",
                            "title": "$PlayMode.SingleLoop"
                        },
                        {
                            "icon": "mdi mdi-shuffle",
                            "fgColor": "#FFFFFF",
                            "title": "$PlayMode.Shuffle"
                        }
                    ]
                },
                "data": {}
            },
            {
                "title": "$Previous.Title",
                "tip": "$Previous.Tip",
                "cid": "com.eniac.neteaseplugin.previous",
                "config": {
                    "keyType": "default",
                    "clickable": true,
                    "platform": ["windows", "mac"]
                },
                "style": {
                    "icon": "mdi mdi-skip-previous",
                    "width": 240
                },
                "data": {}
            },
            {
                "title": "$Next.Title",
                "tip": "$Next.Tip",
                "cid": "com.eniac.neteaseplugin.next",
                "config": {
                    "keyType": "default",
                    "clickable": true,
                    "platform": ["windows", "mac"]
                },
                "style": {
                    "icon": "mdi mdi-skip-next",
                    "width": 240
                },
                "data": {}
            },
            {
                "title": "$PlayPause.Title",
                "tip": "$PlayPause.Tip",
                "cid": "com.eniac.neteaseplugin.playpause",
                "config": {
                    "keyType": "multiState",
                    "platform": ["windows", "mac"]
                },
                "style": {
                    "icon": "mdi mdi-play",
                    "width": 240,
                    "multiStyle": [
                        {
                            "icon": "mdi mdi-play",
                            "fgColor": "#FFFFFF"
                        },
                        {
                            "icon": "mdi mdi-pause",
                            "fgColor": "#E60026"
                        }
                    ]
                },
                "data": {}
            }
        ]
    },
    "local": {
        "en": {
            "PluginName": "Netease Music",
            "Config": {
                "Title": "Netease Music Setup",
                "Step1": {
                    "Title": "Install Netease Music",
                    "Description": "First, you need to install Netease Cloud Music desktop client (version 3.0+).",
                    "Instructions": "1. Download the latest version of Netease Cloud Music\n2. Install it on your computer\n3. Make sure the version is 3.0 or higher",
                    "Button": "Download Netease Music"
                },
                "Step2": {
                    "Title": "Install BetterNCM",
                    "Description": "BetterNCM is a plugin manager for Netease Cloud Music that allows you to extend its functionality.",
                    "Instructions": "1. Visit the BetterNCM website\n2. Download the BetterNCM Installer\n3. Run the installer and follow the prompts\n4. Restart Netease Cloud Music after installation",
                    "Button": "Open BetterNCM Website"
                },
                "Step3": {
                    "Title": "Install FlexLink Plugin",
                    "Description": "Click the button below to install the FlexLink plugin to the BetterNCM plugins folder.",
                    "Button": "Install Plugin",
                    "Idle": "Click the button to start installation",
                    "Installing": "Installing plugin...",
                    "Success": "Plugin installed successfully!",
                    "Error": "Installation failed"
                },
                "Step4": {
                    "Title": "Restart Netease Music",
                    "Description": "Please restart Netease Cloud Music to load the newly installed plugin.",
                    "Instructions": "1. Close Netease Cloud Music completely\n2. Reopen Netease Cloud Music\n3. The FlexLink plugin will be loaded automatically"
                },
                "Step5": {
                    "Title": "Test Connection",
                    "Description": "Verify that the plugin can connect to Netease Cloud Music.",
                    "Button": "Test Connection",
                    "Idle": "Click to test connection",
                    "Testing": "Testing connection...",
                    "Connected": "Connected successfully",
                    "NotConnected": "Not connected. Please make sure Netease Music is running."
                },
                "Next": "Next",
                "Back": "Back",
                "Finish": "Finish",
                "Complete": "Setup completed!"
            },
            "NowPlaying": {
                "Title": "Now Playing",
                "Tip": "Display current playback information",
                "UI": {
                    "ShowTitle": "Show Track Title",
                    "ShowArtist": "Show Artist",
                    "ShowAlbum": "Show Album",
                    "ShowProgress": "Show Progress Bar"
                }
            },
            "Lyric": {
                "Title": "Lyrics",
                "Tip": "Display synchronized lyrics with word-by-word highlight",
                "UI": {
                    "ShowTranslation": "Show Translation",
                    "HighlightWord": "Word-by-word Highlight",
                    "BackgroundColor": "Background Color",
                    "Align": "Alignment",
                    "AlignLeft": "Left",
                    "AlignCenter": "Center",
                    "AlignRight": "Right",
                    "FontSize": "Font Size",
                    "Primary": {
                        "Title": "Current Lyric",
                        "Color": "Text Color",
                        "HighlightColor": "Highlight Color",
                        "PaddingTop": "Distance from Top"
                    },
                    "Secondary": {
                        "Title": "Translation / Next Line",
                        "Color": "Text Color",
                        "PaddingTop": "Distance from Top"
                    },
                    "Padding": {
                        "Title": "Padding",
                        "Horizontal": "Horizontal"
                    }
                }
            },
            "PlayMode": {
                "Title": "Play Mode",
                "Tip": "Toggle play mode (sequential/list loop/single loop/shuffle)",
                "Sequential": "Sequential",
                "ListLoop": "List Loop",
                "SingleLoop": "Single Loop",
                "Shuffle": "Shuffle"
            },
            "Previous": {
                "Title": "Previous",
                "Tip": "Play previous track"
            },
            "Next": {
                "Title": "Next",
                "Tip": "Play next track"
            },
            "PlayPause": {
                "Title": "Play/Pause",
                "Tip": "Toggle playback"
            }
        },
